os from \"axios\";\n\nexport default {\n  data() {\n    return {\n      users: [],\n      selectedUser: null,\n    };\n  },\n  methods: {\n    async fetchUsers() {\n      try {\n        const response = await axios.get(\"https://randomuser.me/api/?results=20\");\n        this.users = response.data.results;\n      } catch (error) {\n        console.error(\"Error fetching users:\", error);\n      }\n    },\n    openUserDetails(user) {\n      this.selectedUser = user;\n      // Trigger modal or page to show user details.\n    }\n  },\n  created() {\n    this.fetchUsers();\n  }\n};\n</script>\n\n<style scoped>\n/* Tailwind CSS styles for the table */\n</style>\n"],"mappings":"AAqCA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMN,KAAK,CAACO,GAAG,CAAC,uCAAuC,CAAC;QACzE,IAAI,CAACL,KAAI,GAAII,QAAQ,CAACL,IAAI,CAACO,OAAO;MACpC,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAAC;MAC/C;IACF,CAAC;IACDE,eAAeA,CAACC,IAAI,EAAE;MACpB,IAAI,CAACT,YAAW,GAAIS,IAAI;MACxB;IACF;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACR,UAAU,CAAC,CAAC;EACnB;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}