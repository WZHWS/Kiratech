{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"UserList\",\n  data() {\n    return {\n      users: [],\n      selectedUser: null,\n      searchQuery: \"\" // search input\n    };\n  },\n  computed: {\n    filteredUsers() {\n      if (!this.searchQuery) return this.users;\n      const query = this.searchQuery.toLowerCase();\n      return this.users.filter(user => (user.name.first + \" \" + user.name.last).toLowerCase().includes(query) || user.email.toLowerCase().includes(query) || user.location.country.toLowerCase().includes(query));\n    }\n  },\n  methods: {\n    async fetchUsers() {\n      try {\n        const response = await axios.get(\"https://randomuser.me/api/?results=7\");\n        this.users = response.data.results;\n      } catch (error) {\n        console.error(\"Error fetching users:\", error);\n      }\n    },\n    openUserDetails(user) {\n      this.selectedUser = user;\n    },\n    closeModal() {\n      this.selectedUser = null;\n    },\n    formatDate(date) {\n      const d = new Date(date);\n      const day = String(d.getDate()).padStart(2, \"0\");\n      const month = d.toLocaleString(\"en-US\", {\n        month: \"short\"\n      });\n      const year = d.getFullYear();\n      return `${day} ${month} ${year}`;\n    },\n    capitalize(word) {\n      if (!word) return \"\";\n      return word.charAt(0).toUpperCase() + word.slice(1).toLowerCase();\n    }\n  },\n  created() {\n    this.fetchUsers();\n  }\n};","map":{"version":3,"names":["axios","name","data","users","selectedUser","searchQuery","computed","filteredUsers","query","toLowerCase","filter","user","first","last","includes","email","location","country","methods","fetchUsers","response","get","results","error","console","openUserDetails","closeModal","formatDate","date","d","Date","day","String","getDate","padStart","month","toLocaleString","year","getFullYear","capitalize","word","charAt","toUpperCase","slice","created"],"sources":["/Users/zihengwang/Desktop/my-project/src/components/UserList.vue"],"sourcesContent":["<template>\n  <div class=\"user-list-wrapper\">\n    <div class=\"max-width\">\n      <!-- Search Box -->\n      <div class=\"search-container\">\n        <input\n          v-model=\"searchQuery\"\n          type=\"text\"\n          placeholder=\"Search users...\"\n          class=\"search-input\"\n        />\n      </div>\n\n      <!-- Table Header -->\n      <div class=\"table-header\">\n        <div>Date</div>\n        <div>Name</div>\n        <div>Gender</div>\n        <div>Country</div>\n        <div>Email</div>\n      </div>\n\n      <!-- User Rows -->\n      <div class=\"user-rows-container\">\n        <div\n          v-for=\"user in filteredUsers\"\n          :key=\"user.email\"\n          class=\"user-row\"\n          @click=\"openUserDetails(user)\"\n        >\n          <div class=\"gray-text\">{{ formatDate(user.dob.date) }}</div>\n          <div\n            :class=\"[\n              'name-cell',\n              selectedUser && selectedUser.email === user.email\n                ? 'selected-name'\n                : ''\n            ]\"\n          >\n            {{ user.name.first }} {{ user.name.last }}\n          </div>\n          <div class=\"gray-text\">{{ capitalize(user.gender) }}</div>\n          <div class=\"black-text\">{{ user.location.country }}</div>\n          <div class=\"gray-text email\">{{ user.email }}</div>\n        </div>\n      </div>\n\n      <!-- Refresh Button -->\n      <div class=\"refresh-wrapper\">\n        <button @click=\"fetchUsers\" class=\"refresh-button\">\n          <i class=\"fas fa-sync-alt\"></i> Refresh\n        </button>\n      </div>\n    </div>\n\n    <!-- MODAL -->\n    <div v-if=\"selectedUser\" class=\"modal-overlay\" @click.self=\"closeModal\">\n      <div class=\"modal-content\">\n        <div class=\"modal-header\">\n          <h3>{{ selectedUser.name.first }} {{ selectedUser.name.last }}</h3>\n          <button @click=\"closeModal\" class=\"close-button\">&times;</button>\n        </div>\n        <div class=\"modal-body\">\n          <p>Date: {{ formatDate(selectedUser.dob.date) }}</p>\n          <p><strong>Status:</strong> Inactive</p>\n          <p><strong>Gender:</strong> {{ capitalize(selectedUser.gender) }}</p>\n          <p><strong>Country:</strong> {{ selectedUser.location.country }}</p>\n          <p><strong>Email:</strong> {{ selectedUser.email }}</p>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"UserList\",\n  data() {\n    return {\n      users: [],\n      selectedUser: null,\n      searchQuery: \"\", // search input\n    };\n  },\n  computed: {\n    filteredUsers() {\n      if (!this.searchQuery) return this.users;\n      const query = this.searchQuery.toLowerCase();\n      return this.users.filter(user =>\n        (user.name.first + \" \" + user.name.last).toLowerCase().includes(query) ||\n        user.email.toLowerCase().includes(query) ||\n        user.location.country.toLowerCase().includes(query)\n      );\n    },\n  },\n  methods: {\n    async fetchUsers() {\n      try {\n        const response = await axios.get(\"https://randomuser.me/api/?results=7\");\n        this.users = response.data.results;\n      } catch (error) {\n        console.error(\"Error fetching users:\", error);\n      }\n    },\n    openUserDetails(user) {\n      this.selectedUser = user;\n    },\n    closeModal() {\n      this.selectedUser = null;\n    },\n    formatDate(date) {\n      const d = new Date(date);\n      const day = String(d.getDate()).padStart(2, \"0\");\n      const month = d.toLocaleString(\"en-US\", { month: \"short\" });\n      const year = d.getFullYear();\n      return `${day} ${month} ${year}`;\n    },\n    capitalize(word) {\n      if (!word) return \"\";\n      return word.charAt(0).toUpperCase() + word.slice(1).toLowerCase();\n    },\n  },\n  created() {\n    this.fetchUsers();\n  },\n};\n</script>\n\n<style scoped>\n/* page layout */\n.user-list-wrapper {\n  background-color: white;\n  min-height: 100vh;\n  padding: 2rem 1rem;\n  margin-top: 50px;\n  width: 50%;\n  margin-left: 20%;\n}\n.max-width {\n  max-width: 1000px;\n  margin: 0 auto;\n}\n\n/* search */\n.search-container {\n  display: flex;\n  justify-content: center;\n  margin-bottom: 1rem;\n}\n.search-input {\n  width: 50%;\n  max-width: 400px;\n  padding: 0.5rem 1rem;\n  border: 1px solid #ddd;\n  border-radius: 4px;\n  font-size: 0.9rem;\n}\n\n/* table header */\n.table-header {\n  display: grid;\n  grid-template-columns: repeat(5, 1fr);\n  background-color: white;\n  color: gray;\n  font-weight: 600;\n  font-size: 0.75rem;\n  padding: 0.75rem 1rem;\n}\n\n/* user rows */\n.user-rows-container {\n  display: flex;\n  flex-direction: column;\n  gap: 1rem;\n  margin-top: 1rem;\n}\n.user-row {\n  display: grid;\n  grid-template-columns: repeat(5, 1fr);\n  background-color: #ffffff;\n  padding: 1rem;\n  border-radius: 0.5rem;\n  box-shadow: 0 1px 4px rgba(0,0,0,0.1);\n  font-size: 0.875rem;\n  cursor: pointer;\n  transition: background-color 0.3s, box-shadow 0.3s;\n}\n.user-row:hover {\n  background-color: #f0faff;\n  box-shadow: 0 4px 8px rgba(0,0,0,0.15);\n}\n.name-cell {\n  color: black;\n  font-weight: 300;\n  transition: color 0.3s;\n}\n.user-row:hover .name-cell {\n  color: #2fc0df;\n}\n.selected-name {\n  color: #2fc0df !important;\n  font-weight: 300; /* stays normal weight */\n}\n\n.gray-text {\n  color: #6b7280;\n}\n.black-text {\n  color: black;\n}\n.email {\n  word-break: break-word;\n}\n\n/* refresh button */\n.refresh-wrapper {\n  display: flex;\n  justify-content: center;\n  margin-top: 2rem;\n}\n.refresh-button {\n  background-color: #2fc0df;\n  color: white;\n  padding: 0.5rem 1.5rem;\n  border-radius: 0.375rem;\n  transition: background-color 0.3s;\n  font-size: 0.875rem;\n  border: none;\n  display: flex;\n  align-items: center;\n  gap: 0.5rem;\n}\n.refresh-button:hover {\n  background-color: #28a6c2;\n}\n\n/* modal */\n.modal-overlay {\n  position: fixed;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  background: rgba(0,0,0,0.5);\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  z-index: 10;\n}\n.modal-content {\n  background: white;\n  padding: 1.5rem;\n  border-radius: 0.5rem;\n  width: 25%;\n  box-shadow: 0 4px 8px rgba(0,0,0,0.2);\n  position: relative;\n}\n.modal-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  font-weight: bold;\n  font-size: 1.1rem;\n  margin-bottom: 1rem;\n}\n.close-button {\n  background: transparent;\n  color: grey;\n  border: none;\n  font-size: 1.2rem;\n  cursor: pointer;\n}\n.modal-body p {\nwidth: 50%;\n  margin: 0.5rem 0;\n  font-size: 0.9rem;\n}\n</style>\n"],"mappings":";;AA2EA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,YAAY,EAAE,IAAI;MAClBC,WAAW,EAAE,EAAE,CAAE;IACnB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,aAAaA,CAAA,EAAG;MACd,IAAI,CAAC,IAAI,CAACF,WAAW,EAAE,OAAO,IAAI,CAACF,KAAK;MACxC,MAAMK,KAAI,GAAI,IAAI,CAACH,WAAW,CAACI,WAAW,CAAC,CAAC;MAC5C,OAAO,IAAI,CAACN,KAAK,CAACO,MAAM,CAACC,IAAG,IAC1B,CAACA,IAAI,CAACV,IAAI,CAACW,KAAI,GAAI,GAAE,GAAID,IAAI,CAACV,IAAI,CAACY,IAAI,EAAEJ,WAAW,CAAC,CAAC,CAACK,QAAQ,CAACN,KAAK,KACrEG,IAAI,CAACI,KAAK,CAACN,WAAW,CAAC,CAAC,CAACK,QAAQ,CAACN,KAAK,KACvCG,IAAI,CAACK,QAAQ,CAACC,OAAO,CAACR,WAAW,CAAC,CAAC,CAACK,QAAQ,CAACN,KAAK,CACpD,CAAC;IACH;EACF,CAAC;EACDU,OAAO,EAAE;IACP,MAAMC,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMpB,KAAK,CAACqB,GAAG,CAAC,sCAAsC,CAAC;QACxE,IAAI,CAAClB,KAAI,GAAIiB,QAAQ,CAAClB,IAAI,CAACoB,OAAO;MACpC,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAAC;MAC/C;IACF,CAAC;IACDE,eAAeA,CAACd,IAAI,EAAE;MACpB,IAAI,CAACP,YAAW,GAAIO,IAAI;IAC1B,CAAC;IACDe,UAAUA,CAAA,EAAG;MACX,IAAI,CAACtB,YAAW,GAAI,IAAI;IAC1B,CAAC;IACDuB,UAAUA