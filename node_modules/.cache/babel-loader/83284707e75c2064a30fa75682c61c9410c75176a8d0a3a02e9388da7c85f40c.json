{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  data() {\n    return {\n      users: [],\n      selectedUser: null\n    };\n  },\n  methods: {\n    async fetchUsers() {\n      try {\n        const response = await axios.get(\"https://randomuser.me/api/?results=20\");\n        this.users = response.data.results;\n      } catch (error) {\n        console.error(\"Error fetching users:\", error);\n      }\n    },\n    openUserDetails(user) {\n      this.selectedUser = user;\n      // Trigger modal or page to show user details.\n    }\n  },\n  created() {\n    this.fetchUsers();\n  }\n};","map":{"version":3,"names":["axios","data","users","selectedUser","methods","fetchUsers","response","get","results","error","console","openUserDetails","user","created"],"sources":["/Users/zihengwang/Desktop/my-project/src/components/UserList.vue"],"sourcesContent":["<template>\n  <div>\n    <button\n      @click=\"fetchUsers\"\n      class=\"bg-[#2fc0df] text-white px-6 py-2 rounded mb-8 hover:bg-[#28a6c2] transition\"\n    >\n      Refresh\n    </button>\n    <div class=\"max-w-6xl mx-auto\">\n      <!-- Header -->\n      <div class=\"grid grid-cols-5 bg-[#e6f7fa] rounded-t-lg text-gray-600 text-xs font-semibold p-4\">\n        <div>Date</div>\n        <div>Name</div>\n        <div>Gender</div>\n        <div>Country</div>\n        <div>Email</div>\n      </div>\n\n      <!-- Rows -->\n      <div>\n        <div\n          v-for=\"(user, index) in users\"\n          :key=\"user.email\"\n          @click=\"openUserDetails(user)\"\n          :class=\"[\n            'grid grid-cols-5 bg-white p-4 mb-4 cursor-pointer transition',\n            'shadow-sm rounded-lg',\n            index === 0 ? 'border border-[#2fc0df]' : 'border border-transparent',\n            'hover:bg-[#f0faff]'\n          ]\"\n        >\n          <div class=\"text-gray-500 text-sm\">{{ new Date(user.dob.date).toLocaleDateString() }}</div>\n          <div>\n            <a href=\"#\" class=\"text-[#2fc0df] text-sm font-semibold hover:underline\">\n              {{ user.name.first }} {{ user.name.last }}\n            </a>\n          </div>\n          <div class=\"text-gray-700 text-sm capitalize font-normal\">{{ user.gender }}</div>\n          <div class=\"text-gray-700 text-sm font-medium\">{{ user.location.country }}</div>\n          <div class=\"text-gray-400 text-sm break-words\">{{ user.email }}</div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  data() {\n    return {\n      users: [],\n      selectedUser: null,\n    };\n  },\n  methods: {\n    async fetchUsers() {\n      try {\n        const response = await axios.get(\"https://randomuser.me/api/?results=20\");\n        this.users = response.data.results;\n      } catch (error) {\n        console.error(\"Error fetching users:\", error);\n      }\n    },\n    openUserDetails(user) {\n      this.selectedUser = user;\n      // Trigger modal or page to show user details.\n    },\n  },\n  created() {\n    this.fetchUsers();\n  },\n};\n</script>\n\n<style scoped>\n/* No special border-radius needed here since rounded-lg on rows */\n</style>\n"],"mappings":"AA+CA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMN,KAAK,CAACO,GAAG,CAAC,uCAAuC,CAAC;QACzE,IAAI,CAACL,KAAI,GAAII,QAAQ,CAACL,IAAI,CAACO,OAAO;MACpC,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAAC;MAC/C;IACF,CAAC;IACDE,eAAeA,CAACC,IAAI,EAAE;MACpB,IAAI,CAACT,YAAW,GAAIS,IAAI;MACxB;IACF;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACR,UAAU,CAAC,CAAC;EACnB;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}